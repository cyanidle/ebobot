<launch>
    <arg name="safe_footpring_radius" default="0.40"/>
    <arg name="debug" default="1"/>
    <node pkg="ebobot" type="global_planer.py" name="motors_info_callback_node" output="log" respawn="true" respawn_delay="10"> 
        <param name = "debug" value = "$(arg debug)"/>
        
    </node>
    <node pkg="ebobot" type="pid_setter.py" name="pid_setter_node" output="log">
        <param name = "debug" value = "$(arg debug)"/>
        <param name = "safe_footprint_radius" value = "$(arg safe_footprint_radius)"/>
    </node>
</launch>